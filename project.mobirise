" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.2\" condition=\"overlay&&bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay&&bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n<div class=\"container\">\n    <div class=\"row justify-content-center\">\n        <div class=\"icons d-flex justify-content-center flex-wrap\"> <!-- Centering icons -->\n            <a href=\"https://on.soundcloud.com/EEijL4aTqLJ2FQCB7\" target=\"_blank\">\n                <span mbr-icon class=\"mbr-iconfont mbr-iconfont-social socicon-soundcloud socicon\" style=\"font-size: 16px;\"></span>\n            </a>\n            <a href=\"https://www.facebook.com/AKousMaTT/\" target=\"_blank\" mbr-if=\"socialCount>1\">\n                <span mbr-icon class=\"mbr-iconfont mbr-iconfont-social socicon-facebook socicon\" style=\"font-size: 16px;\"></span>\n            </a>\n            <a href=\"https://open.spotify.com/artist/6Cpxskc61rpQsNd5gAlzGM\" target=\"_blank\" mbr-if=\"socialCount>2\">\n                <span mbr-icon class=\"mbr-iconfont mbr-iconfont-social socicon-spotify socicon\" style=\"font-size: 16px;\"></span>\n            </a>\n            <a href=\"https://akousmatt.bandcamp.com/\" target=\"_blank\" mbr-if=\"socialCount>3\">\n                <span mbr-icon class=\"mbr-iconfont mbr-iconfont-social socicon-bandcamp socicon\" style=\"font-size: 16px;\"></span>\n            </a>\n            <a href=\"https://www.youtube.com/channel/UCcxgN95YAouEryKK6yevExg\" target=\"_blank\" mbr-if=\"socialCount>4\">\n                <span mbr-icon class=\"mbr-iconfont mbr-iconfont-social socicon-youtube socicon\" style=\"font-size: 16px;\"></span>\n            </a>\n            <a href=\"https://vimeo.com/mobirise\" target=\"_blank\" mbr-if=\"socialCount>5