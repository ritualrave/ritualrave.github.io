" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.2\" condition=\"overlay&&bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay&&bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n<div class=\"container\">\n    <div class=\"row justify-content-center\">\n        <div class=\"icons d-flex justify-content-center flex-wrap\"> <!-- Centering icons -->\n            <a href=\"https://on.soundcloud.com/6C6W11j3zDX1CgCr8\" target=\"_blank\">\n                <span mbr-icon class=\"mbr-iconfont mbr-iconfont-social socicon-soundcloud socicon\" style=\"font-size: 16px;\"></span>\n            </a>\n            <a href=\"https://www.facebook.com/profile.php?id=61552354651051\" target=\"_blank\" mbr-if=\"socialCount>1\">\n                <span mbr-icon class=\"mbr-iconfont mbr-iconfont-social socicon-facebook socicon\" style=\"font-size: 16px;\"></span>\n            </a>\n            <a href=\"https://www.instagram.com/samuraku_/\" target=\"_blank\" mbr-if=\"socialCount>2\">\n                <span mbr-icon class=\"mbr-iconfont mbr-iconfont-social socicon-instagram socicon\" style=\"font-size: 16px;\"></span>\n            </a>\n            <a href=\"https://instagram.com/mobirise\" target=\"_blank\" mbr-if=\"socialCount>3\">\n                <span mbr-icon class=\"socicon-instagram socicon mbr-iconfont mbr-iconfont-social\"></span>\n            </a>\n            <a href=\"https://www.behance.net/Mobirise\" target=\"_blank\" mbr-if=\"socialCount>4\">\n                <span mbr-icon class=\"socicon-behance socicon mbr-iconfont mbr-iconfont-social\"></span>\n            </a>\n            <a href=\"https://vimeo.com/mobirise\" target=\"_blank\" mbr-if=\"socialCount>5\">\n                <span mbr-icon class=\"mbr-iconfont mbr-iconfont-socia