1\" value=\"0.2\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row justify-content-center\">\n            <div class=\"col-12 col-lg-{{textWidth}}\">\n<div class=\"container\">\n  <div style=\"position:relative;text-align:center;min-height:60vh;\">\n    <img src=\"https://i.imgur.com/np0yhKb.png\" alt=\"Image 2\" style=\"position:absolute;top:50%;left:50%;transform:translate(-50%,-50%);animation:rotate 20s linear infinite;\">\n    <img src=\"https://i.imgur.com/MoLYzP2.png\" alt=\"Breathing Image\" style=\"position:absolute;top:50%;left:50%;transform:translate(-50%,-50%);animation:breathe 5s ease-in-out infinite;\">\n    <img src=\"https://i.imgur.com/axkkdyk.png\" style=\"position:absolute;top:50%;left:50%;transform:translate(-50%,-50%);\">\n  </div>\n</div>\n\n<style>\n  @keyframes rotate {\n    from {\n      transform: translate(-50%, -50%) rotate(0deg);\n    }\n    to {\n      transform: translate(-50%, -50%) rotate(360deg);\n    }\n  }\n\n  @keyframes breathe {\n    0% {\n      opacity: 0.1;\n    }\n    50% {\n      opacity: 0.7;\n    }\n    100% {\n      opacity: 0.1;\n    }\n  }\n\n  img {\n    user-select: none;\n  }\n\n  @media (max-width: 767px) {\n    div[style*=\"min-height:60vh;\"] {\n      min-height: auto !important;\n      display: flex;\n      flex-direction: column;\n      justify-content: center;\n      align-items: center;\n      text-align: center;\n      margin: 130px 0; /* Adjust top and bottom margin as needed */\n    }\n\n    img {\n      position: static;\n      transform: none;\n      margin: 0 auto; /* Center horizontally */\n    }\n  }\n</style>\n\n                </div>\n            </div>\n        </div>\n    \n</section>",
